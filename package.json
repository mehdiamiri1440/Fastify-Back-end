{
  "name": "inventory-backend",
  "description": "",
  "main": "src/app.ts",
  "type": "commonjs",
  "scripts": {
    "start": "node -r ts-node/register -r module-alias/register main.ts",
    "dev": "node -r dotenv/config -r ts-node/register -r module-alias/register main.ts | pino-pretty",
    "dev:watch": "node --watch -r dotenv/config -r ts-node/register -r module-alias/register main.ts | pino-pretty",
    "format": "prettier --loglevel=error --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:quiet": "eslint . --quiet",
    "check": "tsc --noEmit",
    "ci": "npm run format:check && npm run lint:quiet && npm run check",
    "seed": "node -r dotenv/config -r ts-node/register -r module-alias/register ./node_modules/@jorgebodega/typeorm-seeding/dist/cli.js seed -d src/DataSource.ts src/seeders/*.ts",
    "test": "DOTENV_CONFIG_PATH=.env.test jest --runInBand",
    "test:coverage": "DOTENV_CONFIG_PATH=.env.test jest --collectCoverage --runInBand",
    "migration:generate": "node -r dotenv/config -r ts-node/register -r module-alias/register node_modules/typeorm/cli migration:generate -d=src/DataSource.ts",
    "migration:run": "node -r dotenv/config -r ts-node/register -r module-alias/register node_modules/typeorm/cli migration:run -d=src/DataSource.ts",
    "prepare": "if [ -d .git ]; then husky install; fi"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
